[
    {
        "method": "post",
        "path": "/api/v1/checkout",
        "handler": "Checkout.createSession",
        "body": {
            "products": [
                {
                    "product_id": "[product_id]",
                    "amount": "[amount]"
                }
            ]
        }
    },
    {
        "method" : "get",
        "path" : "/api/v1/checkout/:checkout_id",
        "handler" : "Checkout.getOne"
    }
]