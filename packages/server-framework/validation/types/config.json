{
    "type": {
        "regex": "^\\[[a-zA-Z_]*\\]$",
        "error": "Invalid type"
    },
    "type_array": {
        "regex": "^<[a-zA-Z_]*>$",
        "error": "Invalid array type"
    },
    "optional_type_array": {
        "regex" : "^\\([a-zA-Z_]*\\)$",
        "error": "Invalid optional type array"
    },
    "path_variable": {
        "regex": "^:[a-zA-Z_]*",
        "error": "Invalid path variable"
    },
    "config_key": {
        "regex": "\\??[a-zA-Z]{1,30}",
        "error": "Configuration key does not match specifications"
    },
    "method": {
        "regex": "\\b(post|get|put|delete)\\b",
        "error": "Method not recognized"
    },
    "path": {
        "regex": ".*",
        "error": "Invalid path"
    },
    "handler": {
        "regex": ".*",
        "error": "Invalid path"
    }
}