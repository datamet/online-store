[
    {
        "method" : "post",
        "path" : "/api/v1/users",
        "handler" : "Users.createOne"
    },
    {
        "method" : "post",
        "path" : "/api/v1/auth/local",
        "handler" : "Local.authenticate",
        "body" : {
            "email" : "[email]",
            "password" : "[password]"
        }
    },
    {
        "method" : "delete",
        "path" : "/api/v1/auth/local",
        "handler" : "Local.unauthenticate"
    },
    {
        "method" : "post",
        "path" : "/api/v1/auth/google",
        "handler" : "Google.authenticate"
    },
    {
        "method" : "delete",
        "path" : "/api/v1/auth/google",
        "handler" : "Google.unauthenticate"
    }
]