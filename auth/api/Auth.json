[
    {
        "method" : "post",
        "path" : "/api/v1/users",
        "handler" : "Users.createOne",
        "body" : {
            "first_name" : "[first_name]",
            "?last_name" : "[last_name]"
        }
    },
    {
        "method" : "get",
        "path" : "/api/v1/login",
        "handler" : "Users.login"
    },
    {
        "method" : "post",
        "path" : "/api/v1/auth/local",
        "handler" : "Local.authenticate"
    },
    {
        "method" : "delete",
        "path" : "/api/v1/auth/local",
        "handler" : "Local.unauthenticate"
    },
    {
        "method" : "post",
        "path" : "/api/v1/auth/google",
        "qurery" : {
            "id_token" : "[id_token]"
        },
        "handler" : "Google.authenticate"
    }
]