[
    {
        "method" : "post",
        "path" : "/auth/users",
        "handler" : "Users.createOne",
        "body" : {
            "username" : "[username]"
        }
    },
    {
        "method" : "get",
        "path" : "/auth/login",
        "handler" : "Users.login"
    },
    {
        "method" : "post",
        "path" : "/auth/local",
        "handler" : "Local.authenticate"
    },
    {
        "method" : "delete",
        "path" : "/auth/local",
        "handler" : "Local.unauthenticate"
    },
    {
        "method" : "post",
        "path" : "/auth/google",
        "qurery" : {
            "id_token" : "[id_token]"
        },
        "handler" : "Google.authenticate"
    }
]